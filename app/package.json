{
  "name": "app",
  "version": "1.0.1",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test:e2e:ios": "detox test --configuration ios.sim.debug",
    "test:e2e:android": "detox test --configuration android.emu.debug",
    "test:e2e:ios:build": "detox build --configuration ios.sim.debug",
    "test:e2e:android:build": "detox build --configuration android.emu.debug",
    "test:e2e:ios:clean": "detox clean-framework-cache && detox build-framework-cache",
    "test:e2e:android:clean": "detox clean-framework-cache && detox build-framework-cache",
    "test:maestro": "maestro test maestro/",
    "test:maestro:ios": "maestro test maestro/ --device \"iPhone 17 Pro\"",
    "test:maestro:android": "maestro test maestro/ --device \"Pixel_5_API_33\"",
    "test:appium": "wdio wdio.conf.js",
    "test:appium:ios": "wdio wdio.conf.js --spec ./e2e-appium/**/*.spec.js",
    "appium:server": "appium",
    "auto:ios": "node scripts/auto-build-and-test.js ios",
    "auto:android": "node scripts/auto-build-and-test.js android",
    "auto:ios:skip-build": "node scripts/auto-build-and-test.js ios --skip-build",
    "auto:android:skip-build": "node scripts/auto-build-and-test.js android --skip-build",
    "monitor:ios": "node scripts/monitor-build-and-test.js"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-google-signin/google-signin": "^16.0.0",
    "@react-native-picker/picker": "^2.11.4",
    "@react-navigation/bottom-tabs": "^7.8.5",
    "@react-navigation/native": "^7.1.20",
    "@react-navigation/native-stack": "^7.6.3",
    "@tanstack/react-query": "^5.90.10",
    "axios": "^1.13.2",
    "babel-preset-expo": "~54.0.0",
    "expo": "^54.0.25",
    "expo-clipboard": "~8.0.7",
    "expo-linear-gradient": "~15.0.7",
    "expo-status-bar": "~3.0.8",
    "i18next": "^25.6.2",
    "lucide-react-native": "^0.554.0",
    "opencc-js": "^1.0.5",
    "react": "19.1.0",
    "react-i18next": "^16.3.3",
    "react-native": "0.81.5",
    "react-native-markdown-display": "^7.0.2",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "^5.6.2",
    "react-native-screens": "^4.18.0",
    "react-native-sse": "^1.2.1",
    "react-native-svg": "^15.15.0",
    "react-native-toast-message": "^2.3.3",
    "react-native-webview": "13.15.0",
    "react-native-worklets": "^0.5.1",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/react": "~19.1.0",
    "@wdio/cli": "^9.20.1",
    "@wdio/local-runner": "^9.20.1",
    "@wdio/mocha-framework": "^9.20.1",
    "@wdio/spec-reporter": "^9.20.0",
    "appium": "^3.1.1",
    "appium-xcuitest-driver": "^10.5.1",
    "babel-plugin-module-resolver": "^5.0.2",
    "detox": "^20.46.0",
    "jest": "^30.2.0",
    "jest-circus": "^30.2.0",
    "typescript": "~5.9.2",
    "wdio-appium-service": "^0.2.3"
  },
  "private": true
}
